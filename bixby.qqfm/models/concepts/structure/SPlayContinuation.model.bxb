structure (SPlayContinuation) {
  description (播放第几个的输出)
  property (ordinal) {
    type (common.Ordinal)
    min(Required) max (One)
  }
  property (singer) {
    type (Singer)
    min (Optional) max (Many)
  }
  property (sAlbum) {
    type (SAlbum)
    min (Optional) max (Many)
  }
  property (sShow) {
    type (SShow)
    min (Optional) max (Many)
  }
  property (sBroadcast) {
    type (SBroadcast)
    min (Optional) max (Many)
  }
}
